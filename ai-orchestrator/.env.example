# AI Orchestrator Environment Variables

# Flask Configuration
FLASK_ENV=development
PORT=5001
SECRET_KEY=your-secret-key-here

# Database Configuration (Shared with main app)
DATABASE_URL=postgresql://user:password@host:port/database

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_TEMPERATURE=0.7

# LangChain Configuration
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=your-langchain-api-key-here

# Main App Configuration
MAIN_APP_URL=http://localhost:5000
MAIN_APP_API_KEY=your-main-app-api-key-here

# Agent Configuration
ENABLE_SELF_HEALING=true
ENABLE_CODE_MONITORING=true
ENABLE_WORKOUT_OPTIMIZATION=true
ENABLE_PROGRESS_MONITORING=true
ENABLE_SCHEDULING_INTELLIGENCE=true

# Monitoring
PROMETHEUS_PORT=9090
LOG_LEVEL=INFO
